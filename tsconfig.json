{
  "extends": "@sindresorhus/tsconfig",
  "compilerOptions": {
	"target": "es2018",
	"lib": ["es6", "dom"],
	"strict": true,
	"declaration": false,
	"noImplicitAny": false,
	"noImplicitThis": true,
	"strictNullChecks": true,
	"allowUnusedLabels": true,

  },
  "include": [
	"source"
  ],
  "rules": {
	"no-unused-vars": "off"
  }
}
